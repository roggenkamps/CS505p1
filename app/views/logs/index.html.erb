<p id="notice"><%= notice %></p>

<h1>Logs</h1>

<table>
  <thead>
    <tr>
      <th>Date time</th>
      <th>User</th>
      <th>Subject</th>
      <th>Operation</th>
      <th>Object</th>
      <th>Parameters</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.created_at %></td>
        <td><%= log.user %></td>
        <td><%= log.subject %></td>
        <td><%= log.operation %></td>
        <td><%= log.object %></td>
        <td><%= log.parameters %></td>
        <td><%= link_to 'Show', log %></td>
      </tr>
    <% end %>
  </tbody>
</table>
